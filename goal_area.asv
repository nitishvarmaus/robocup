function goal_area
mf=1;
    draw_line([2 2].*mf,[2.5 5.500].*mf); %y
    draw_line([9000*mf 9000*mf],[2500*mf 5.500*mf]); %y
    draw_line([1000*mf 2000*mf],[5500 5500].*mf); %x
    draw_line([9000 10000].*mf,[5500 5500].*mf); %x
    draw_line([9000 10000].*mf,[2500 2500].*mf); %x
    draw_line([1000 2000].*mf,[2500 2500].*mf); %x
end

